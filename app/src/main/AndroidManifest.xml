            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.filemanager.fileexplorer.myfile.cleanmaster.free.lookscreen.ChangePassActivity"
