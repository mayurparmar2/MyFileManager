            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoDisplay" />

        <service android:name="com.filemanager.fileexplorer.myfile.cleanmaster.free.Rambooter.antivirus.KillNotificationsService" />

        <receiver
