            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <service
            android:name="com.filemanager.fileexplorer.myfile.cleanmaster.free.Rambooter.antivirus.MonitorShieldService"
            android:enabled="true" />


