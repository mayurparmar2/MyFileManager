<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="enableProgressAnimation" format="boolean">
    </attr>
    <attr name="progressBackgroundColor" format="color">
    </attr>
    <attr name="progressBackgroundStrokeWidth" format="dimension">
    </attr>
    <attr name="progressCap">
        <enum name="round" value="0" />
        <enum name="butt" value="1" />
    </attr>
    <attr name="progressColor" format="color">
    </attr>

    <attr name="progressStrokeWidth" format="dimension">
    </attr>
    <attr name="startAngle" format="reference|integer">
    </attr>

    <declare-styleable name="TabView">
        <attr name="tabv_colorMain" format="color" />

        <attr name="tabv_colorSub" format="color" />
        <attr name="tabv_duration" format="integer" />
        <attr name="tabv_padding" format="dimension" />
        <attr name="tabv_paddingSide" format="dimension" />
        <attr name="tabv_textSize" format="dimension" />
        <attr name="tabv_title" format="string" />

    </declare-styleable>
    <declare-styleable name="ProgressWheel">
        <attr name="barColor" format="color"/>
        <attr name="barLength" />
        <attr name="barWidth" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="dimension"/>
        <attr name="delayMillis" format="integer"/>
        <attr name="radius" format="dimension"/>
        <attr name="rimColor" format="color"/>
        <attr name="rimWidth" format="dimension"/>
        <attr name="spinSpeed" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
    <attr name="cpv_contourSize" format="dimension">
    </attr>
    <attr name="direction">
        <enum name="clockwise" value="0" />
        <enum name="counterclockwise" value="1" />
    </attr>

</resources>
